<Window x:Class="Klient_Server.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Klient_Server"
        mc:Ignorable="d"
        Title="MainWindow" Height="476" Width="800">
    <Grid>
        <Button Content="Start Server" HorizontalAlignment="Left" Margin="91,390,0,0" VerticalAlignment="Top" Height="34" Width="152" Click="Button_Click"/>
        <Button Content="Stop Server" HorizontalAlignment="Center" Margin="0,390,0,0" VerticalAlignment="Top" Height="34" Width="152" Click="Button_Click_1"/>
        <Label Content="Server Log" HorizontalAlignment="Left" Margin="91,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="log" HorizontalAlignment="Center" Height="344" Margin="0,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="617"/>

    </Grid>
</Window>
